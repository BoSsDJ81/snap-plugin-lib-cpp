{
	"version": 1,
	"schedule": {
		"type": "simple",
		"interval": "1s"
	},
	"max-failures": 10,
	"workflow": {
		"collect": {
			"metrics": {
				"/intel/cpp/mock/random_number/one": {},
				"/intel/cpp/mock/random_number/two": {}
			},
			"config": {
					"/intel/cpp/mock/random_number/one": {
							"password": "silly goose"
					}
			},
			"process": [{
				"plugin_name": "graffiti",
				"config": {
					"tags": "one,two,three"
				},
				"publish": [{
						"plugin_name": "log",
						"config": {
								"path": "/tmp/log_pub.log"
						}
				}]
			}]
		}
	}
}
